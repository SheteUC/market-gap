{
  "name": "marketgap-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "demo": "npx ts-node --project tsconfig.node.json src/scripts/demo.ts",
    "test:agents": "npx ts-node --project tsconfig.node.json src/scripts/quickTest.ts",
    "test:persistence": "npx ts-node --project tsconfig.node.json src/scripts/testPersistence.ts",
    "agents:init": "npx ts-node --project tsconfig.node.json -e \"require('./src/scripts/demo.ts').runDemo()\"",
    "agents:test": "npx ts-node --project tsconfig.node.json -e \"require('./src/utils/agentManager.ts').agentManager.initializeAllAgents().then(console.log)\""
  },
  "dependencies": {
    "@ant-design/colors": "^7.1.0",
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/icons": "^5.4.0",
    "@ant-design/nextjs-registry": "^1.0.2",
    "@letta-ai/letta-client": "^0.1.132",
    "@letta-ai/vercel-ai-sdk-provider": "^0.0.8",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "ai": "^4.3.16",
    "antd": "^5.21.4",
    "dayjs": "^1.11.13",
    "next": "14.2.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.2.30",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.6",
    "ts-node": "^10.9.2"
  }
}
